{
    "name": "codefit/wolt-api-client",
    "description": "PHP client for Wolt API integration",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wolt\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wolt\\Api\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
} 